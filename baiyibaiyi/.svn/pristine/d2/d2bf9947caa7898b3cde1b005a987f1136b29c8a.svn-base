package www.qisu666.com.utils

import android.util.Log

/**
 * Created by wujiancheng on 2017/10/9.
 */
inline fun <reified T> T.logI(log: Any) {
    Log.i(T::class.simpleName, "-----" + log.toString())
}

inline fun <reified T> T.logE(log: Any) {
    Log.e(T::class.simpleName, "-----" + log.toString())
}
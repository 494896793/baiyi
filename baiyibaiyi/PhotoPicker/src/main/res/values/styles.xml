<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--分割线-->
    <style name="__picker_DivideLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/__picker_color_gray_e5e5e5</item>
    </style>
    <!-- 从底部弹出动画 -->
    <style name="__picker_AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/__picker_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/__picker_push_bottom_out</item>
    </style>
</resources>
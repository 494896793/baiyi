<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="android:Theme.Light">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ProgressBar_MiNi" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">50dip</item>
        <item name="android:minHeight">3dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/progress_horizontal</item>
    </style>

    <style name="TextView_Subtitle_Dark_Gray" parent="TextView_Subtitle">
        <item name="android:textColor">@color/text_dark_gray</item>
    </style>

    <style name="Divide_Vertical_Full">
        <item name="android:background">@color/bg_primary</item>
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="LL_Normal">
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/default_padding</item>
        <item name="android:paddingRight">@dimen/default_padding</item>
        <item name="android:paddingTop">@dimen/default_padding_tnb</item>
        <item name="android:paddingBottom">@dimen/default_padding_tnb</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="Divide_Horizontal">
        <item name="android:background">@color/text_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_1dp</item>
        <item name="android:layout_marginLeft">@dimen/divide_margin_lnr</item>
        <item name="android:layout_marginRight">@dimen/divide_margin_lnr</item>
    </style>

    <style name="TextView_Subtitle_Dark_Gray_MarginLeft" parent="TextView_Subtitle_Dark_Gray">
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <!--LinearLayout样式-->
    <style name="LL_Item">
        <item name="android:background">@drawable/selector_linear_btn</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/default_padding</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:clickable">true</item>
    </style>

    <!--启动APP -->
    <style name="LaunchStyle" parent="AppTheme">
        <!--设置透明背景-->
        <item name="android:windowIsTranslucent">true</item>
        <!--设置Appliction背景-->
        <item name="android:windowBackground">@drawable/background_start</item>
    </style>

    <style name="ProgressBar_Update" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">50dip</item>
        <item name="android:minHeight">8dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/update_progress_horizontal</item>
    </style>

    <style name="MyAppTheme" parent="android:Theme.Holo.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/top_background_default</item>
    </style>

    <style name="RatingBarStyleMain" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_rating_bar_main</item>
        <item name="android:indeterminateDrawable">@drawable/star_rating_bar_main</item>
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">22dip</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="RatingBarStyleYellow" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_rating_bar_yellow_light</item>
        <item name="android:indeterminateDrawable">@drawable/star_rating_bar_yellow_light</item>
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">21dip</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="RatingBarYellow" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_rating_bar_yellow_light</item>
        <item name="android:indeterminateDrawable">@drawable/star_rating_bar_yellow_light</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="RatingBarStyleYellowSmall" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_rating_bar_yellow</item>
        <item name="android:indeterminateDrawable">@drawable/star_rating_comments_yellow_light
        </item>
        <item name="android:minHeight">18dp</item>
        <item name="android:maxHeight">22dp</item>
        <item name="android:thumb">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="RatingBarChargeDetial" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_rating_bar_yellow</item>
        <item name="android:minHeight">22dip</item>
        <item name="android:maxHeight">22dp</item>
        <item name="android:thumb">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="AhwAlertDialog" parent="@android:style/Theme.Dialog">

        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:windowNoTitle">true</item>
        <!-- 背景是否变模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ivPayMoney">
        <item name="android:layout_width">17dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_marginLeft">16dp</item>
    </style>

    <style name="ivChoosePay">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="VerifyCodeStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:numeric">integer</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#25282d</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textSize">36sp</item>
        <item name="android:maxLength">1</item>
        <item name="android:cursorVisible">false</item>
    </style>

    <!--分割线-->
    <style name="DivideLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/color_gray_e5e5e5</item>
    </style>

    <style name="DivideLineShadowStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/bg_shadow</item>
    </style>
    <!--登录按钮不可点击背景-->
    <style name="LoginButtonUnenableStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">47dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@drawable/bg_blue_radius_login_unenable</item>
        <item name="android:clickable">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--登录按钮可点击背景-->
    <style name="LoginButtonEnableStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">47dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@drawable/selector_primary_btn</item>
        <item name="android:clickable">false</item>
        <item name="android:textColor">@color/main_background</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--注册后，立即登录按钮的灰色-->
    <style name="ToLoginButtonGrayStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">47dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@drawable/bg_gray_radius</item>
        <item name="android:clickable">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--蓝底白字-->
    <style name="BlueBgWhiteTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">47dp</item>
        <item name="android:background">@color/color_blue_02b2e4</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="Ellipsize">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="SwitchButtonStyleMD" parent="SwitchButtonMD">
        <item name="android:paddingLeft">15dp</item>
    </style>
    <!-- 从底部弹出动画 -->
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="TrafficViolationTextLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_gray_999999</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">13dp</item>
    </style>

    <style name="TrafficViolationTextRight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_black_333333</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">13dp</item>
    </style>

    <style name="ListViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/main_background</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:divider">@color/transparent</item>
        <item name="android:dividerHeight">0px</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="tvPayType">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="iv_wallet">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="tv_wallet">
        <item name="android:layout_width">wrap_content</item>

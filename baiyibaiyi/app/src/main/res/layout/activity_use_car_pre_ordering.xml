<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_background"
    android:orientation="vertical">

    <include android:id="@+id/title_bar" layout="@layout/title_white" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <www.qisu666.com.view.CarInfoOrderView
            android:id="@+id/carInfoOrderView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <www.qisu666.com.view.ViewPagerWithTitleViews
            android:id="@+id/viewPagerWithTitleView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_21dp">

        </www.qisu666.com.view.ViewPagerWithTitleViews>

    </LinearLayout>
</LinearLayout>